<%= simple_form_for model, method: verb, url: url do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>
    <%= f.input :title, label: "Name your project" %> <hr>
    <%= f.input :description, label: "Describe your project"%> <hr>
    <%= f.input :project_date %> <hr>
    <%= f.input :contributors %> <hr>
    <%= f.input :link %> <hr>
  <%= f.button :submit %>
<% end %>
