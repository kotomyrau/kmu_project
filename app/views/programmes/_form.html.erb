<%= simple_form_for model, method: verb, url: url do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>
    <%= f.input :name, label: "Name of programme" %> <hr>
    <%= f.input :skill, collection: 1..10 %> <hr>
  <%= f.button :submit %>
<% end %>
